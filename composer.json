{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "astrotomic/laravel-translatable": "^11.6",
        "darryldecode/cart": "~4.0",
        "fideloper/proxy": "^4.0",
        "jackiedo/dotenv-editor": "1.*",
        "laravel/framework": "5.8.  *",
        "laravel/passport": "^7.5",
        "lcobucci/jwt": "3.3.2",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mcamara/laravel-localization": "^1.4",
        "nwidart/laravel-modules": "^6.2.0",
        "paquettg/php-html-parser": "^2.2",
        "php-ffmpeg/php-ffmpeg": "^0.16.0",
        "pragmarx/countries": "^0.7.0",
        "rap2hpoutre/laravel-log-viewer": "^1.6",
        "spatie/laravel-backup": "^6.10",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-sluggable": "^2.2",
        "spatie/valuestore": "^1.2",
        "unisharp/laravel-filemanager": "dev-master",
        "vsch/laravel-translation-manager": "*",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "module-dir": "Custom"
    },
    "autoload": {
        "psr-4": {
          "App\\": "app/",
          "Modules\\": "Modules/",
          "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
